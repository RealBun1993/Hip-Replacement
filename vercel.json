{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/index.html",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/generate_data.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python@3.12"
    }
  },
  "installCommand": "pip install --upgrade setuptools wheel && apt-get update && apt-get install -y python3-distutils",
  "buildCommand": "bash build.sh"
}
