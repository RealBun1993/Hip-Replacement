{
  "version": 2,
  "builds": [
    {
      "src": "api/generate_data.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/generate_data.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ],
  // 添加这一行注释以触发重新构建
  "comment": "Force clear cache and rebuild"
}
