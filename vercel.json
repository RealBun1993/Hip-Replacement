{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python",
      "config": { "runtime": "python3.8" }
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/generate_data.py"
    },
    {
      "src": "/(.*)",
      "dest": "/static/index.html"
    }
  ]
}
