{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/generate_data.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python@3.12"
    }
  },
  "buildCommand": "bash build.sh"
}

