{
  "version": 2,
  "builds": [
    { "src": "api/**/*.py", "use": "@vercel/python", "config": { "quiet": true } },
    { "src": "static/index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}
